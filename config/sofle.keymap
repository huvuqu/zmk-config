                              &trans        &trans        &kp DOT       &kp UNDER     &kp EQUAL        &trans        &trans        &trans        &trans        &trans
//                           ├─────────────┼─────────────┼─────────────┼─────────────┼─────────────┤  ├─────────────┼─────────────┼─────────────┼─────────────┼─────────────┤
            >;
            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN &inc_dec_kp PG_UP PG_DN>; 
        };

        fun_layer {
            display-name = "fun";
            bindings = <
